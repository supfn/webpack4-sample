{
  "presets": ["react", "env"],
  "env": {
    "development": {
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]]
    }
  },
  "comments": false,
  "plugins": [
    "transform-runtime",
    "syntax-dynamic-import",
  ]
}